{
  "name": "bf-ui",
  "version": "1.0.0",
  "description": "User interface for the Beachfront project",
  "scripts": {
    "build": "webpack",
    "create-ssl-certs": "openssl req -newkey rsa:2048 -keyout .development_ssl_certificate.key -sha256 -x509 -days 99999 -nodes -out .development_ssl_certificate.pem -passin pass:secret -passout pass:secret -subj '/C=US/ST=Unknown/L=Unknown/O=Beachfront/OU=Development/CN=localhost'",
    "clean": "rimraf report dist beachfront.zip",
    "lint": "tslint '{src,test}/**/*.{ts,tsx}' -t verbose",
    "test": "karma start",
    "test:ci": "NODE_ENV=ci karma start --single-run --browsers Chrome,Firefox --reporters progress,coverage,junit",
    "typings:install": "typings install",
    "watch": "webpack-dev-server --inline --colors --history-api-fallback --https --cert .development_ssl_certificate.pem --key .development_ssl_certificate.key"
  },
  "author": "RadiantBlue Technologies",
  "license": "Apache-2.0",
  "engines": {
    "node": "^5.7.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.0",
    "enzyme": "2.6.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "istanbul-instrumenter-loader": "1.1.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-junit-reporter": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "mocha": "3.2.0",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "8.2.0",
    "postcss-loader": "1.1.1",
    "react-addons-test-utils": "15.3.1",
    "rimraf": "2.5.4",
    "sinon": "2.0.0-pre",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-loader": "1.2.2",
    "tslint": "4.0.2",
    "tslint-loader": "3.2.0",
    "typescript": "2.1.4",
    "typings": "2.0.0",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "axios": "0.15.2",
    "core-js": "2.4.1",
    "font-awesome": "4.6.1",
    "lodash": "4.17.2",
    "moment": "2.17.0",
    "openlayers": "3.19.1",
    "react": "15.3.1",
    "react-dom": "15.3.1"
  }
}
